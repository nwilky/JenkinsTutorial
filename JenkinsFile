pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'echo "Hello from Jenkins!"'
            }
        }
        stage('Artifact Generation') {
            steps{
                sh 'echo "We are generating artifacts for ${BUILD_NUMBER}" > output.txt'
            }
        }
        stage('Staging') {
            steps {
                sh 'echo "Deploy to Staging Environment!"'
            }
        }
    }
}
